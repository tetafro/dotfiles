{
    "breadcrumbs.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        100
    ],
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "on",
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitblame.infoMessageFormat": "${time.custom,YYYY-MM-DD} ${commit.summary} [${commit.hash_short, 8}]",
    "githd.blameView.enabled": false,
    "githd.logView.commitsCount": 100,
    "githd.logView.displayExpressStatus": false,
    "go.buildOnSave": "package",
    "go.buildTags": "e2e",
    "go.enableCodeLens": {
        "references": false,
        "runtest": false
    },
    "go.formatTool": "gofmt",
    "go.formatFlags": [
        "-s"
    ],
    "go.useLanguageServer": true,
    "nasc-touchbar.addCursorBelow": false,
    "nasc-touchbar.togglePanel": false,
    "nasc-touchbar.showCommands": false,
    "nasc-touchbar.showReferences": true,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "/usr/local/bin/flake8",
    "python.linting.pylintEnabled": false,
    "python.formatting.provider": "none",
    "todohighlight.keywords": [
        {
            "text": "NOTE:",
            "color": "white",
            "backgroundColor": "rgb(200, 160, 50)"
        },
        {
            "text": "WARNING:",
            "color": "white",
            "backgroundColor": "rgb(200, 160, 50)"
        },
        {
            "text": "CAUTION:",
            "color": "white",
            "backgroundColor": "rgb(200, 160, 50)"
        },
        {
            "text": "TODO:",
            "color": "white",
            "backgroundColor": "rgb(190, 80, 80)"
        }
    ],
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
        "statusBar.background": "#303030",
        "statusBar.noFolderBackground": "#222225",
        "statusBar.debuggingBackground": "#511f1f",
        "editorRuler.foreground": "#333333",
        "editorIndentGuide.background": "#333333",
        "editor.selectionHighlightBackground": "#333333ee"
    },
    "workbench.colorTheme": "Tomorrow Night",
    "workbench.startupEditor": "newUntitledFile"
}
